{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext"
    },
    "coverPhoto": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "postedAt": {
      "type": "date"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "blogs"
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "admin::user"
    },
    "externalFile": {
      "type": "component",
      "repeatable": true,
      "component": "blog.external-files"
    }
  }
}
